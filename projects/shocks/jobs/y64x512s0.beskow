#!/bin/bash
#SBATCH -A 2018-2-41
#SBATCH -J 512s10
#SBATCH --output=%J.out
#SBATCH --error=%J.err
#SBATCH -t 0-23:00:00
#SBATCH --nodes 16

# activate threading
export OMP_NUM_THREADS=1
export PYTHONDONTWRITEBYTECODE=true
export HDF5_USE_FILE_LOCKING=FALSE

# go to working directory
cd $RUNKODIR/projects/shocks/

srun -n 512 python pic.py --conf y64x512s0.ini
